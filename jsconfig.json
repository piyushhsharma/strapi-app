{
  "compilerOptions": {
    "rootDir": ".",
    "module": "Node16",
    "moduleResolution": "node16",
    "target": "ES2020",
    "skipLibCheck": true,
    "strict": false
  },
  "exclude": [
    "**/node_modules/*",
    ".git",
    ".npm",
    "**/.cache/*",
    "**/.tmp/*",
    "**/dist/*",
    "**/build/*",
    "**/*.test.js",
    "**/*.test.api.js",
    "**/examples/getstarted/*",
    "**/examples/kitchensink/*"
  ]
}
